*/*.c    \
*/*.lst  \
*/*.inp  \
*/*.w    \
*/*.j    \
*/*.l    \
*/*.h    \
*/*.log  \
*/*.dat  \
*/tr.lsj \
*/summry \
*/delete

cd ..

echo "\n -- bin/"
cd bin
